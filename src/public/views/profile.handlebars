<div class='content w-full'>
	<h1 class='p-relative'>Profile</h1>
	<div class='profile-page m-20'>
		<!-- Start Overview -->
		<div class='overview bg-white rad-10 d-flex align-center'>
			<div class='avatar-box txt-c p-20'>
				<img
					class='rad-half mb-10'
					src='{{account.generalInformation.image}}'
					alt=''
				/>
				<h3 class='m-0'>{{account.generalInformation.fullname}}</h3>
				<p class='c-grey mt-10'>Level
					{{account.generalInformation.level}}</p>
				<div class='level rad-6 bg-eee p-relative'>
					<span style='width: 70%'></span>
				</div>
				<div class='rating mt-10 mb-10'>
					<i class='fa-solid fa-star c-orange fs-13'></i>
					<i class='fa-solid fa-star c-orange fs-13'></i>
					<i class='fa-solid fa-star c-orange fs-13'></i>
					<i class='fa-solid fa-star c-orange fs-13'></i>
					<i class='fa-solid fa-star c-orange fs-13'></i>
				</div>
				<p
					class='c-grey m-0 fs-13'
				>{{account.generalInformation.rating}} Rating</p>
			</div>
			<div class='info-box w-full txt-c-mobile'>
				<!-- Start Information Row -->
				<div class='box p-20 d-flex align-center'>
					<h4 class='c-grey fs-15 m-0 w-full'>
						General Information
					</h4>
					<div class='fs-14'>
						<span class='c-grey'>Full Name</span>
						<span>{{account.generalInformation.fullname}}</span>
					</div>
					<div class='fs-14'>
						<span class='c-grey'>Gender:</span>
						<span>{{account.generalInformation.gender}}</span>
					</div>
					<div class='fs-14'>
						<span class='c-grey'>Country:</span>
						<span>{{account.generalInformation.country}}</span>
					</div>
					<div class='fs-14'>
						<label>
							<input
								class='toggle-checkbox'
								type='checkbox'
								checked
							/>
							<div class='toggle-switch'></div>
						</label>
					</div>
				</div>
				<!-- End Information Row -->

				<!-- Start Information Row -->
				<div class='box p-20 d-flex align-center'>
					<h4 class='c-grey w-full fs-15 m-0'>
						Personal Information
					</h4>
					<div class='fs-14'>
						<span class='c-grey'>Email:</span>
						<span>{{account.personalInformation.email}}</span>
					</div>
					<div class='fs-14'>
						<span class='c-grey'>Phone:</span>
						<span>{{account.personalInformation.phoneNumber}}</span>
					</div>
					<div class='fs-14'>
						<span class='c-grey'>Date Of Birth:</span>
						<span>{{account.personalInformation.dateOfBirth}}</span>
					</div>
					<div class='fs-14'>
						<label>
							<input class='toggle-checkbox' type='checkbox' />
							<div class='toggle-switch'></div>
						</label>
					</div>
				</div>
				<!-- End Information Row -->

				<!-- Start Information Row -->
				<div class='box p-20 d-flex align-center'>
					<h4 class='c-grey w-full fs-15 m-0'>
						Job Information
					</h4>
					<div class='fs-14'>
						<span class='c-grey'>Title:</span>
						<span>{{account.jonInformation.title}}</span>
					</div>
					<div class='fs-14'>
						<span class='c-grey'>Programming Language:</span>
						<span>{{account.jonInformation.ProgrammingLang}}</span>
					</div>
					<div class='fs-14'>
						<span class='c-grey'>Years Of Experience:</span>
						<span>{{account.jonInformation.yearsOfExperice}}</span>
					</div>
					<div class='fs-14'>
						<label>
							<input
								class='toggle-checkbox'
								type='checkbox'
								checked
							/>
							<div class='toggle-switch'></div>
						</label>
					</div>
				</div>
				<!-- End Information Row -->

				<!-- Start Information Row -->
				<div class='box p-20 d-flex align-center'>
					<h4 class='c-grey w-full fs-15 m-0'>
						Billing Information
					</h4>
					<div class='fs-14'>
						<span class='c-grey'>Payment Method:</span>
						<span
						>{{account.billingInformation.paymentMethod}}</span>
					</div>
					<div class='fs-14'>
						<span class='c-grey'>Email:</span>
						<span>{{account.billingInformation.email}}</span>
					</div>
					<div class='fs-14'>
						<span class='c-grey'>Subscription:</span>
						<span>{{account.billingInformation.subscription}}</span>
					</div>
					<div class='fs-14'>
						<label>
							<input class='toggle-checkbox' type='checkbox' />
							<div class='toggle-switch'></div>
						</label>
					</div>
				</div>
				<!-- End Information Row -->
			</div>
		</div>
		<!-- End Overview -->
		<!-- Start Other Data -->
		<div class='other-data d-flex gap-20'>
			<div class='skills-card p-20 bg-white rad-10 mt-20'>
				<h2 class='mt-0 mb-10'>My Skills</h2>
				<p class='mt-0 mb-20 c-grey fs-15'>
					Complete Skills List
				</p>
				<ul class='m-0 txt-c-mobile'>
					{{#each mySkills}}
						<li>
							{{#each this}}
								<span>{{this}}</span>
							{{/each}}
						</li>

					{{/each}}
				</ul>
			</div>
			<div class='activities p-20 bg-white rad-10 mt-20'>
				<h2 class='mt-0 mb-10'>Latest Activities</h2>
				<p class='mt-0 mb-20 c-grey fs-15'>
					Latest Activities Done By The User
				</p>
				<div class='activity d-flex align-center txt-c-mobile'>
					<img src='images/activity-01.png' alt='' />
					<div class='info'>
						<span class='d-block mb-10'>Store</span>
						<span
							class='c-grey'
						>{{latestActivities.store.activity}}</span>
					</div>
					<div class='date'>
						<span
							class='d-block mb-10'
						>{{latestActivities.store.time}}</span>
						<span
							class='c-grey'
						>{{latestActivities.store.day}}</span>
					</div>
				</div>
				<div class='activity d-flex align-center txt-c-mobile'>
					<img src='images/activity-02.png' alt='' />
					<div class='info'>
						<span class='d-block mb-10'>Academy</span>
						<span
							class='c-grey'
						>{{latestActivities.academy.activity}}</span>
					</div>
					<div class='date'>
						<span
							class='d-block mb-10'
						>{{latestActivities.academy.time}}</span>
						<span
							class='c-grey'
						>{{latestActivities.academy.day}}</span>
					</div>
				</div>
				<div class='activity d-flex align-center txt-c-mobile'>
					<img src='images/activity-03.png' alt='' />
					<div class='info'>
						<span class='d-block mb-10'>Badges</span>
						<span
							class='c-grey'
						>{{latestActivities.badges.activity}}</span>
					</div>
					<div class='date'>
						<span
							class='d-block mb-10'
						>{{latestActivities.badges.time}}</span>
						<span
							class='c-grey'
						>{{latestActivities.badges.day}}</span>
					</div>
				</div>
				<div class='activity d-flex align-center txt-c-mobile'>
					<img src='images/activity-01.png' alt='' />
					<div class='info'>
						<span class='d-block mb-10'>Store</span>
						<span
							class='c-grey'
						>{{latestActivities.acheivment.activity}}</span>
					</div>
					<div class='date'>
						<span
							class='d-block mb-10'
						>{{latestActivities.acheivment.time}}</span>
						<span
							class='c-grey'
						>{{latestActivities.acheivment.day}}</span>
					</div>
				</div>
			</div>
		</div>
		<!-- End Other Data -->
	</div>
</div>